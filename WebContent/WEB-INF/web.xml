<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
  id="WebApp_ID" version="2.5">
  <display-name>tasks</display-name>
  <welcome-file-list>
    <welcome-file>Login.html</welcome-file>
  </welcome-file-list>

  <servlet>
    <servlet-name>monitor</servlet-name>
    <servlet-class>task.web.MonitorServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>monitor</servlet-name>
    <url-pattern>/monitor</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>login</servlet-name>
    <servlet-class>task.web.LoginServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>login</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>logout</servlet-name>
    <servlet-class>task.web.LogoutServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>logout</servlet-name>
    <url-pattern>/logout</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>overview</servlet-name>
    <servlet-class>task.web.OverviewServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>overview</servlet-name>
    <url-pattern>/overview</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>customer</servlet-name>
    <servlet-class>task.web.CustomerServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>customer</servlet-name>
    <url-pattern>/customer</url-pattern>
  </servlet-mapping>

</web-app>